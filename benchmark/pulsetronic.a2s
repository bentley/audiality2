HatNoise(P V W spread decay)
{
	struct { wtosc }
	!e V
	w W; p P; a e
	!da (V / decay)
	decay {
		p (rand spread + P); a e; -e da; d 1
	}
}
Hat(V=1 L=1)
{
	HatNoise 9000f (V * .5) triangle .6 (50 * L)
	HatNoise 7000f (V * .3) triangle .5 (300 * L)
	HatNoise 5000f (V * .15) triangle .4 (250 * L)
	HatNoise 3000f (V * .1) triangle .3 (200 * L)
}


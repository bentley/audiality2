c0(P A W rmax dl)
{
	struct { wtosc; panmix }
	pan (rand 1 - .5)
	w W; a A; set a; tr P
	!rmh (rmax * .25)
	200 {	p (rand rmax - rmh); *rmax .997; *rmh .997; *a .995; d dl }
	wg (a - .0001) {
		100 {	p (rand rmax - rmh); d dl }
		*a .9; *rmax .99; *rmh .99
	}
}
Cymbal(P V=1)
{
	c0 (P + 5.3) V saw 2 .3
	c0 (P + 4) (V * .5) saw 2 .4
	c0 (P + 3) (V * .3) square 2 .5
	c0 (P + 2.3) (V * .2) sine 4 .6
}

Explo0(P V=1 Wave Attack Decay)
{
	struct { wtosc; panmix }
	pan (rand 1 - .5)
	+P tr; w Wave; !x 1.5; a V; -P 3; tr Attack; !D 1
	200 {	*tr .98; p (P + rand x); +x .01; d D; +D .005 }
	800 {	*tr .98; p (P + rand x); -x .0025; *a Decay; d D; +D .005 }
	tr P
	wg (a - .001) {	rand p x; *a Decay; d 2.5 }
}
Explosion(P V=1)
{
	+tr P
	Explo0 1 (V * .2) sine 2 .995
	Explo0 -1 V sine 4 .998
}

Reed0(P V=1 Wave)
{
	struct { wtosc }
	!P0 tr; +P tr; w Wave; p P; a 0; !er .1; !vib 0; !tvib .005
.rt	wg (V + a - .001) {
		tr (vib * 6 + rand .01)
		12 {	-tr vib; +p (P - p * .3); +a (V - a * er); d 5 }
		12 {	+tr vib; +p (P - p * .3); +a (V - a * er); d 5 }
		+vib (tvib - vib * .1)
	}
	a 0; d 5
	1(NV) {
		V NV
		if NV {
			vib .005; tvib .005; er .1; wake rt
		} else {
			tvib .02; er .03
		}
	}
	2(NP) { P (P0 + NP) }
}
Reed(P V=1)
{
	struct { inline; panmix }
	1:Reed0 (P + 1.01) (V * .3) saw
	2:Reed0 P (V * .4) saw
	1(NV) { 1<1 (NV * .2); 2<1 (NV * .5) }
	2(NP) { 1<2 (NP + .01); 2<2 NP }
}

Whistle(P V=1)
{
	struct { wtosc; panmix }
	pan .5; set pan
	!P0 tr; +P tr; w triangle; p P; a 0; !er .1; !vib 0; !tvib .005
.rt	wg (V + a - .001) {
		tr (vib * 6 + rand .01)
		12 {	-tr vib; +p (P - p * .3); +a (V - a * er); d 5 }
		12 {	+tr vib; +p (P - p * .3); +a (V - a * er); d 5 }
		+vib (tvib - vib * .2)
	}
	a 0; d 5
	1(NV) {
		V NV
		if NV {
			vib .005; tvib .005; er .1; wake rt
		} else {
			tvib .02; er .02
		}
	}
	2(NP) { P (P0 + NP) }
}

SoftArp(P V=1 Maj=0)
{
	struct { wtosc; panmix }
	sa0(PP VV)
	{
		pan (rand 1 - .5); set pan
		p PP; a VV;	td .01
		a 0;		td .49
	}
	w square
	a V; +tr P; !aa a
	7 {
		sa0 12n aa
		sa0 0 aa
		sa0 (3n + Maj) aa
		sa0 7n aa
		*aa .75
		sa0 (15n + Maj) aa
		sa0 (3n + Maj) aa
		sa0 7n aa
		sa0 12n aa
		*aa .75
		sa0 19n aa
		sa0 7n aa
		sa0 12n aa
		sa0 (15n + Maj) aa
		+tr 1; *aa .75
	}
}

Bleep2(P V=1 Wave)
{
	struct { wtosc }
	w Wave; p (P - .5); phase (rand 1); a V; set a
	10 {
		-p .05; d 3
	}
	wg 1 {
		p (P + rand .02 - .01)
		d (50 + rand 50)
	}
.stop	a 0; td 1
	td 2.5
	a (V * .4); td .1
	a 0; td 1
	td 2.5
	a (V * .1); td .1
	a 0; td 1
	1() { force stop }
}
BleepLead(P V=1)
{
	struct { inline; panmix }
	Bleep2 (P - .01) (V * .25) saw
	Bleep2 (P + .01) (V * .25) saw
	Bleep2 (P + .995) (V * .2) saw
	Bleep2 (P + 2.005) (V * .15) saw
	wg 1 { td 1000 }
.stop	*<1
	1() { force stop }
}

Str0(P V Attack Decay Detune)
{
	struct { wtosc; panmix }
	pan (Detune * 50)
	w saw; a 0; !Ta (V * .1); phase (rand 1)
	+P Detune
	p P; !er (Attack * .1)
.rt	wg (V + a - .001) {
		+p (P - p * .4 + rand .02 - .01)
		+a (Ta - a * er); Ta (V * rand .5)
		d (rand 5 + 2.5)
	}
	a 0; d 5
	1(NV) {
		ifg (NV - V) {
			V NV; er (Attack * .1); wake rt
		} else {
			V NV; er (Decay * .1)
		}
	}
	2(NP) { P (NP + Detune) }
}
EStrings(P V=1 Attack=.3 Decay=.2)
{
	!Voices (3 - P quant 1)
	ifl (Voices - 2) {
		Voices 2
	}
	def Spread .03
	!detune (0 - (Spread / 2))
	Voices {
		*:Str0 P V Attack Decay detune
		+detune (Spread / Voices)
	}
	1(NV) { *<1 NV }
	2(NP) { *<2 NP }
}

Blepp(P V=1)
{
	struct { wtosc; filter12; panmix }
	pan (rand .5 - .25)
	lp 1; bp 1; hp .5
	q .01; set q; cutoff (P + 3); set cutoff
	w saw; a (V * 1.1); set a; p (P - 1);	cutoff (P + 2);	d 5
	+p 1; *a .5;			cutoff (P + 1);	d 15
	-p 1; a 0;			cutoff P;	d 100
}

PulseBass(P V=1)
{
	struct {
		wtosc
		wtosc o2
		wtosc o3
		filter12
		panmix
	}
	!detune (.005 * (1 - P))
	ifl (detune - .002) { detune .002 }
	w saw; a (V * .5); set a; p (P - detune)
	o2.w saw; o2.a (V * .2); set o2.a; o2.p (P + detune + 1)
	o3.w sine; o3.a (V * .8); set o3.a; o3.p (P)
	lp 1; bp 1; hp .3
	q .1; set q; cutoff (P + 4); set cutoff
	cutoff (P + 3); q .01;	d 30
	cutoff (P + 1); q .002;	d 100
	cutoff (P + .5);	d 300
	a 0; o2.a 0; o3.a 0;	d 500
	end
.rel	a 0; o2.a 0; o3.a 0; cutoff (P - 1); q 0; d 100
	d 500
	1() { force rel }
}

